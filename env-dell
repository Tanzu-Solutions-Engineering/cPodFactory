VCENTER_CLUSTER="DELL Cluster"
TEMPLATE_VM="template-cPodRouter"
TEMPLATE_ESX="template-ESX67U3"
#TEMPLATE_ESX="template-ESX70beta"
TEMPLATE_FILER="template-cPodFiler"
DATASTORE=${DATASTORE:="DELL-VSAN"}
