EXTRA_DIR=extra

NSX=10.1.0.156
NSX_ADMIN="admin"
NSX_TRANSPORTZONE="TransportZone"

ISO_BANK_SERVER="10.1.0.51"
ISO_BANK_DIR="/VNXeDatastore/ISO-Bank"

VCENTER=10.1.0.19
VCENTER_ADMIN="cpod@dell.lab"
